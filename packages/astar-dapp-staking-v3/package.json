{
  "name": "@astar-network/dapp-staking-v3",
  "version": "1.0.0",
  "description": "Astar dApp staking v3 library",
  "main": "build/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/models/library/index.js",
    "./utils": "./build/utils/index.js"
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "docs": "typedoc"
  },
  "peerDependencies": {
    "@polkadot/api": "^12.2.1"
  },
  "devDependencies": {
    "typedoc": "^0.26.7",
    "typedoc-plugin-markdown": "^4.2.8"
  },
  "typedocOptions": {
    "entryPoints": [
      "src"
    ],
    "out": "docs",
    "plugin": [
      "typedoc-plugin-markdown"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/*.test.ts"
    ],
    "disableSources": true
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
